{
  "name": "libram",
  "version": "0.0.4",
  "description": "JavaScript helper library for KoLmafia",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Loathing-Associates-Scripting-Society/libram",
  "author": "Samuel Gaus <sam@gaus.co.uk>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn run tsc",
    "clean": "rm -f {src,dist}/*.js {src,dist}/*.d.ts {src,dist}/**/*.js {src,dist}/**/*.d.ts",
    "format": "yarn run prettier --write src/**/* examples/**/*",
    "lint": "yarn run eslint src --ext .ts",
    "prepare": "patch-package",
    "prepublishOnly": "yarn run build"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/compat-data": "^7.12.7",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "lint-staged": ">=10",
    "node-fetch": "^2.6.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "kolmafia": "^1.0.3",
    "node-html-parser": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "prettier --write"
  },
  "sideEffects": false
}